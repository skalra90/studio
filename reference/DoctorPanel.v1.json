                },
                "examples": {
                  "If finance token is missing": {
                    "value": {
                      "message": "\"finance_token\" is required",
                      "code": "parameter_missing"
                    }
                  },
                  "Invalid finance token is provided": {
                    "value": {
                      "message": "Invalid finance token",
                      "code": "invalid_finance_token"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
