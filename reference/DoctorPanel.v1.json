    "/customer/finance/status": {
      "get": {
        "summary": "Get finance token status",
        "tags": [
          "Customers"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "code": {
                      "type": "string"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "status": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "Used token": {
                    "value": {
                      "message": "Request executed successfully",
                      "code": "success",
                      "data": {
                        "status": "unused_token",
                        "message": "This finance token is not used yet"
                      }
                    }
                  },
                  "Successful Payment": {
                    "value": {
                      "message": "Request executed successfully",
                      "code": "success",
                      "data": {
                        "status": "payment_successful",
                        "message": "Payment successful for this token"
                      }
                    }
                  },
                  "Failed Payment": {
                    "value": {
                      "message": "Request executed successfully",
                      "code": "success",
                      "data": {
                        "status": "payment_failed",
                        "message": "Payment failed with error message 'Your card has expired.'"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
